<div id="admin">
  <h2>Posts</h2>
  <%= link_to 'New Post', new_post_path %>
  <h3>Unpublished Posts</h3>
  <% Post.pending.each do |post| %>
    <%= link_to post.title, edit_post_path(post) %>
  <% end %>

  <h2>Photos</h2>
  <%= link_to 'New Photo', new_photo_path %>
  <h3>Unpublished Photos</h3>
  <% Photo.pending.each do |photo| %>
    <%= render partial: 'photos/thumb', locals: { photo: photo } %>
  <% end %> 
</div>
